{
  "KafkaConsumer": {
    "project": "dotnet/KafkaConsumer/KafkaConsumer.csproj",
    "envSource": "./keycloak/.env.kafka-consumer",
    "localOverrides": {
      "REDIS_URL": "localhost:6379",
      "KAFKA_BROKER_LIST": "localhost:9092",
      "AUTH_API_URL": "https://localhost:5555",
      "OTLP_ENDPOINT_URL": "http://localhost:4317",
      "GRAPHQL_API_URL": "http://localhost:5900/graphql",
      "LOCALIZATION_API_URL": "http://localhost:8888",
      "CACHE_SERVER_ADDRESS": "http://localhost:7777",
      "STRIPE_API_URL": "http://localhost:4243",
      "OIDC_SERVER_AUTH_URL": "https://keycloak.local:8443",
      "Oidc__ServerUrl": "https://keycloak.local:8443"
    },
    "oidc": {
      "fromEnvFile": true,
      "map": {
        "Oidc__ClientId": "Oidc:ClientId",
        "Oidc__ClientSecret": "Oidc:ClientSecret",
        "Oidc__Realm": "Oidc:Realm",
        "Oidc__ServerUrl": "Oidc:ServerUrl"
      }
    }
  }
}
